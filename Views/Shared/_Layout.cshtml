<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Basic SEO Meta Tags -->
    <title>Mohamed Ibrahim - Software Developer & System Architect</title>
    <meta name="description" content="Mohamed Ibrahim, a highly skilled software developer and architect with 15+ years of experience in .NET, React.js, SQL Server, SharePoint, and cloud technologies.">
    <meta name="keywords" content="Software Developer, .NET, React.js, SharePoint, SQL Server, Blazor, Web Development, Cloud Computing, Oman, Mohamed Ibrahim">
    <meta name="author" content="Mohamed Ibrahim">

    <!-- Open Graph (OG) Meta Tags for Social Media Sharing -->
    <meta property="og:title" content="Mohamed Ibrahim - Software Developer & System Architect">
    <meta property="og:description" content="Experienced software developer with expertise in .NET, React.js, SharePoint, and cloud computing.">
    <meta property="og:image" content="https://www.moa7amed.com/profile-image.jpeg"> <!-- Replace with your actual profile image URL -->
    <meta property="og:url" content="https://www.moa7amed.com">
    <meta property="og:type" content="website">

    <!-- Twitter Card Tags for Better Sharing -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Mohamed Ibrahim - Software Developer">
    <meta name="twitter:description" content="15+ years of experience in software development, system architecture, and project leadership.">
    <meta name="twitter:image" content="https://www.moa7amed.com/profile-image.jpeg"> <!-- Replace with actual profile image -->
    <!-- Canonical URL to Avoid Duplicate Content Issues -->
    <link rel="canonical" href="https://www.moa7amed.com">

    <!-- Structured Data (JSON-LD) for Better Indexing -->
    <script type="application/ld+json">
        {
          "context": "https://schema.org",
          "type": "Person",
          "name": "Mohamed Ibrahim",
          "url": "https://www.moa7amed.com",
          "image": "https://www.moa7amed.com/profile-image.jpeg",
          "jobTitle": "Senior Software Developer & System Architect",
          "worksFor": {
            "type": "Organization",
            "name": "SSL-OMAN"
          },
          "sameAs": [
            "https://www.linkedin.com/in/moa7amed",
            "https://github.com/moa7amed",
            "https://twitter.com/moa7amed",
              "https://www.kaggle.com/mohamedramadan2040"
          ]
        }
    </script>

    <!-- Favicon for Branding -->
    <link rel="icon" href="https://www.moa7amed.com/favicon.ico" type="image/x-icon">

    <!-- Robots Meta Tag (Allows Search Engines to Index the Page) -->
    <meta name="robots" content="index, follow">

    <!-- Additional SEO Enhancements -->
    <meta name="google-site-verification" content="your-google-verification-code"> <!-- If using Google Search Console -->





    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    @* <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Resume.Web.styles.css" asp-append-version="true" /> *@

    @*     <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true" /> *@
    <link rel="stylesheet" href="~/css/normalize.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/transition-animations.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/owl.carousel.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/animate.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />


    <script src="~/js/jquery-2.1.3.min.js" asp-append-version="true"></script>
    <script src="~/js/modernizr.custom.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

</head>
<body>

    <!-- Loading animation -->
    <div class="preloader">
        <div class="preloader-animation">
            <div class="dot1"></div>
            <div class="dot2"></div>
        </div>
    </div>
    <!-- /Loading animation -->


    <div id="page" class="page">
        <!-- Header -->
        <header id="site_header" class="header mobile-menu-hide">
            <div class="my-photo">
                <img src="images/my_photo.jpeg" alt="image">
                <div class="mask"></div>
            </div>

            <div class="site-title-block">
                <h1 class="site-title">Mohamed Ibrahim</h1>
                <p class="site-description">AI Architect</p>
            </div>

            <!-- Navigation & Social buttons -->
            <div class="site-nav">
                <!-- Main menu -->
                <ul id="nav" class="site-main-menu">
                    <!-- About Me Subpage link -->
                    <li>
                        <a class="pt-trigger" href="#home" data-animation="58" data-goto="1">Home</a><!-- href value = data-id without # of .pt-page. Data-goto is the number of section -->
                    </li>
                    <!-- /About Me Subpage link -->
                    <!-- About Me Subpage link -->
                    <li>
                        <a class="pt-trigger" href="#about_me" data-animation="59" data-goto="2">About me</a>
                    </li>
                    <!-- /About Me Subpage link -->
                    <li>
                        <a class="pt-trigger" href="#resume" data-animation="60" data-goto="3">Resume</a>
                    </li>
                    @*  <li>
                        <a class="pt-trigger" href="#portfolio" data-animation="61" data-goto="4">Portfolio</a>
                    </li> 
                    <li>
                        <a class="pt-trigger" href="#blog" data-animation="58" data-goto="5">Blog</a>
                    </li>
                    <li>
                        <a class="pt-trigger" href="#contact" data-animation="59" data-goto="6">Contact</a>
                    </li> *@
                </ul>
                <!-- /Main menu -->
                <!-- Social buttons -->
                <ul class="social-links">
                    <li>
                        <a class="tip social-button" href="https://www.kaggle.com/mohamedramadan2040" title="My Kaggle Profile">
                            <svg xmlns="http://www.w3.org/2000/svg" width="12" height="15" viewBox="0 0 320 512" preserveAspectRatio="xMidYMid meet">
                                <path fill="#888" d="M304.2 501.5L158.4 320.3 298.2 185c2.6-2.7 1.7-10.5-5.3-10.5h-69.2c-3.5 0-7 1.8-10.5 5.3L80.9 313.5V7.5q0-7.5-7.5-7.5H21.5Q14 0 14 7.5v497q0 7.5 7.5 7.5h51.9q7.5 0 7.5-7.5v-109l30.8-29.3 110.5 140.6c3 3.5 6.5 5.3 10.5 5.3h66.9q5.3 0 6-3z" />
                            </svg>

                        </a>
                    </li>




                    <li><a class="tip social-button" href="https://www.linkedin.com/in/moa7amed/" title="Linkedin"><i class="fa fa-linkedin"></i></a></li>


                </ul>
                <!-- /Social buttons -->
            </div>
            <!-- Navigation & Social buttons -->
        </header>
        <!-- /Header -->
        <!-- Mobile Header -->
        <div class="mobile-header mobile-visible">
            <div class="mobile-logo-container">
                <div class="mobile-site-title">Mohamed Ibrahim</div>
            </div>

            <a class="menu-toggle mobile-visible">
                <i class="fa fa-bars"></i>
            </a>
        </div>
        <!-- /Mobile Header -->
        <!-- Main Content -->
        <div id="main" class="site-main">
            @RenderBody()
        </div>
        <!-- /Main Content -->
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @* <script src="~/js/site.js" asp-append-version="true"></script> *@


    <script src="~/js/page-transition.js" asp-append-version="true"></script>
    <script src="~/js/imagesloaded.pkgd.min.js" asp-append-version="true"></script>
    <script src="~/js/validator.js" asp-append-version="true"></script>
    <script src="~/js/jquery.shuffle.min.js" asp-append-version="true"></script>
    <script src="~/js/masonry.pkgd.min.js" asp-append-version="true"></script>
    <script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.magnific-popup.min.js" asp-append-version="true"></script>
    <script src="~/js/jquery.hoverdir.js" asp-append-version="true"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>





    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
